{
  "manifest_version": 3,
  "name": "Custom Shortcut Extension",
  "version": "1.0",
  "description": "Bind a custom shortcut to a button on a webpage.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
